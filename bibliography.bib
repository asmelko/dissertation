  keywords        = {Graphics processing units, Real-time systems, Mathematical model, Computer architecture, Correlation, Digital signal processing, Signal processing algorithms, Digital Signal Processing, Cross-Correlation, Parallel Computing, GPU Computing, CUDA},
}

